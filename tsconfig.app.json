{
  "extends": "./tsconfig.json",
  "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "@app/*": [
          "src/app/*"
        ],
        "@service/*": [
          "src/app/service/*"
        ]
      },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
